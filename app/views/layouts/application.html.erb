<!DOCTYPE html>
<html>
<head>
		<!-- Basic Page Needs
		================================================== -->
  <meta charset="utf-8">
  <title>AngelPM - Ruby</title>
  <!-- Device Specific Metas
  ================================================== -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
 <!-- CSS
 ================================================== -->
  <!-- web font  -->
  <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,800" %>
  <!-- plugin css  -->
  <%= stylesheet_link_tag "js-plugin/animation-framework/animate.min.css" %>

  <%= stylesheet_link_tag "js-plugin/jquery/isotope/css/style.css" %>
  <%= stylesheet_link_tag "js-plugin/flexslider/flexslider.css"%>
  <%= stylesheet_link_tag "js-plugin/pageSlide/jquery.pageslide.css"%>
  <!-- Owl carousel-->
  <%= stylesheet_link_tag "js-plugin/owl.carousel/owl-carousel/owl.carousel.css" %>
  <%= stylesheet_link_tag "js-plugin/owl.carousel/owl-carousel/owl.theme.css" %>

  <%= javascript_include_tag "modernizr-2.6.1.min.js" %>
  <%= javascript_include_tag "jquery" %>


</head>

<body data-spy="scroll" data-target="#scrollTarget" data-offset="150" id="fullScreen">
<!-- Preloader -->
	<!--<div id="preloader">
	  <div id="status">
		<img src="images/theme-pics/loader.gif" alt=""/>
		<h1 class="noSubtitle">Just a moment please...</h1>
	  </div>
	</div>-->
<!-- Preloader -->
<!-- Primary Page Layout
		================================================== -->
<!-- globalWrapper -->
<div id="globalWrapper" class="localscroll">
  <div class="nav navbar-default navbar-fixed-top navbar-header scrollMenu" id="mainHeader" role="banner">
	<div class="nav navbar-header" id="navigation">
	<h1 class="navbar-brand" href="#">AngelPM Ruby</h1>
	  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
		<i class="sr-only">Toggle navigation</i>
		<i class="icon-bar"></i>
		<i class="icon-bar"></i>
		<i class="icon-bar"></i>
	  </button>
	  </div> <!-- required to pull menu items right, but leave brand pulled left-->
	  <div class="nav collapse navbar-collapse pull-right" id="scrollTarget">
		<ul class="nav navbar-nav pull-right">
		  <li class="active"><%= link_to "Home", "#homeFullScreen" %></li>
		  <li><%= link_to "Features", '#features' %></li>
		  <li><%= link_to "About", '#about' %></li>
		  <li><%= link_to "Plans", '#plans' %></li>
		  <li><%= link_to "Team", '#team' %></li>
		  <li><%= link_to "Contact", '#contact' %></li>
		  <li><%= link_to "Login", '#' %></li>
		  <li><%= link_to "Sign Up", '#' %></li>
		</ul>
	  </div>
	</div>


  <!-- end navbar -->

  <!-- page content -->


  <%= yield %>


  	<!-- end page content -->
</div>
	<!-- End Document
================================================== -->

<%= javascript_include_tag "js-plugin/respond/respond.min.js" %>
<%= javascript_include_tag "js-plugin/jquery/1.8.3/jquery.min.js" %>
<%= javascript_include_tag "js-plugin/jquery-ui/jquery-ui-1.8.23.custom.min.js" %>
<%= javascript_include_tag "js-plugin/easing/jquery.easing.1.3.js" %>

<%= javascript_include_tag "js-plugin/jquery/isotope/jquery.isotope.min.js" %>
<%= javascript_include_tag "js-plugin/neko-contact-ajax-plugin/js/jquery.form.js" %>
<%= javascript_include_tag "js-plugin/neko-contact-ajax-plugin/js/jquery.validate.min.js" %>
<%= javascript_include_tag "js-plugin/magnific-popup/jquery.magnific-popup.min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.scrollTo-1.4.3.1-min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.localscroll-1.2.7-min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.stellar.min.js" %>
<%= javascript_include_tag "js-plugin/pageSlide/jquery.pageslide-custom.js" %>
<%= javascript_include_tag "js-plugin/smoothscroll/SmoothScroll.js" %>
<%= javascript_include_tag "js-plugin/jquery.sharrre-1.3.4/jquery.sharrre-1.3.4.min.js" %>
<%= javascript_include_tag "js-plugin/owl.carousel/owl-carousel/owl.carousel.min.js"%>
<!-- Custom  -->
<%= javascript_include_tag "custom.js" %>

</body>
</html>
