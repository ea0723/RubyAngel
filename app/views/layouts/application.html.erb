<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>AngelPM - Ruby</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag "stylesheet", type: "text/css", href: "js-plugin/animation-framework/animate.css" %>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag "stylesheet", type:"text/css", src:"js-plugin/magnific-popup/magnific-popup.css" %>
  <%= stylesheet_link_tag "stylesheet", type:"text/css", src:"js-plugin/isotope/css/style.css" %>
  <%= stylesheet_link_tag "stylesheet", type:"text/css", src:"js-plugin/flexslider/flexslider.css"%>
  <%= stylesheet_link_tag "stylesheet", type:"text/css", scr:"js-plugin/pageSlide/jquery.pageslide.css"%>
  <!-- Owl carousel-->
  <%= stylesheet_link_tag "stylesheet", src:"js-plugin/owl.carousel/owl-carousel/owl.carousel.css" %>
  <%= stylesheet_link_tag "stylesheet", src:"js-plugin/owl.carousel/owl-carousel/owl.theme.css" %>

  <%= stylesheet_link_tag "stylesheet", type:"text/css", src:"http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,800" %>

  <%= javascript_include_tag "modernizr-2.6.1.min.js" %>

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>

<body data-spy="scroll" data-target="#scrollTarget" data-offset="150" id="fullScreen">
<!-- Preloader -->
	<!--<div id="preloader">
	  <div id="status">
		<img src="images/theme-pics/loader.gif" alt=""/>
		<h1 class="noSubtitle">Just a moment please...</h1>
	  </div>
	</div>-->
<!-- Preloader -->
<!-- Primary Page Layout
		================================================== -->
<!-- globalWrapper -->
<div id="globalWrapper" class="localscroll">
  <div class="navbar-wrapper" id="mainHeader">
	<div class="container">
	  <div class="navbar navbar-default navbar-fixed-top scrollMenu" role="navigation">
		<div class="container">
		  <h1 class="navbar-brand">AngelPM Ruby</h1>
		  <div class="navbar-header">
			<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
			  <span class="sr-only">Toggle navigation</span>
			  <span class="icon-bar"></span>
			  <span class="icon-bar"></span>
			  <span class="icon-bar"></span>
			</button>
		  </div>
		  <div class="navbar-collapse collapse">
			<ul class="nav navbar-nav pull-right">
			  <li class="active"><%= link_to "Home", root_path %><i class="icon-home-outline"></i></li>
			  <li><%= link_to "Features", '#services' %><i class="icon-thumbs-up"></i></li>
			  <li><%= link_to "About", '#about' %><i class="icon-star"></i></li>
			  <li><%= link_to "Plans", '#plans' %><i class="icon-star"></i></li>
			  <li><%= link_to "Team", '#team' %><i class="icon-users"></i></li>
			  <li><%= link_to "Contact", '#contactSlice' %><i class="icon-mail"></i></li>
			  <li><%= link_to "Login", '#' %><i class="icon-star"></i></li>
			  <li><%= link_to "Sign Up", '#' %><i class="icon-star"></i></li>
			</ul>
		  </div>
		</div>
	  </div>
	</div>
  </div>

  <!-- end navbar -->

  <!-- page content -->


  <%= yield %>


  	<!-- end page content -->
</div>
	<!-- End Document
================================================== -->

<%= javascript_include_tag "js-plugin/respond/respond.min.js" %>
<%= javascript_include_tag "js-plugin/jquery/1.8.3/jquery.min.js" %>
<%= javascript_include_tag "js-plugin/jquery-ui/jquery-ui-1.8.23.custom.min.js" %>
<%= javascript_include_tag "js-plugin/easing/jquery.easing.1.3.js" %>
<%= javascript_include_tag "js-plugin/flexslider/jquery.flexslider-min.js" %>
<%= javascript_include_tag "js-plugin/isotope/jquery.isotope.min.js" %>
<%= javascript_include_tag "js-plugin/neko-contact-ajax-plugin/js/jquery.form.js" %>
<%= javascript_include_tag "js-plugin/neko-contact-ajax-plugin/js/jquery.validate.min.js" %>
<%= javascript_include_tag "js-plugin/magnific-popup/jquery.magnific-popup.min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.scrollTo-1.4.3.1-min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.localscroll-1.2.7-min.js" %>
<%= javascript_include_tag "js-plugin/parallax/js/jquery.stellar.min.js" %>
<%= javascript_include_tag "js-plugin/pageSlide/jquery.pageslide-custom.js" %>
<%= javascript_include_tag "js-plugin/smoothscroll/SmoothScroll.js" %>
<%= javascript_include_tag "js-plugin/jquery.sharrre-1.3.4/jquery.sharrre-1.3.4.min.js" %>
<%= javascript_include_tag "js-plugin/owl.carousel/owl-carousel/owl.carousel.min.js"%>
<!-- Custom  -->
<%= javascript_include_tag "custom.js" %>

</body>
</html>
